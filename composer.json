{
    "name": "gregoryxg/laravel-fullcalendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Shawn Tunney",
            "email": "shawn.tunney@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "*"
    },
    "autoload": {
        "psr-4": {
            "gregoryxg\\LaravelFullcalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "gregoryxg\\LaravelFullcalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "gregoryxg\\LaravelFullcalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
